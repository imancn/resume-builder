    "eslint": "^9.33.0",
    "eslint-plugin-vue": "^10.4.0",
    "netlify-cli": "^23.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^7.1.0",
    "vue-tsc": "^3.0.5"
  }
